{
  "compilerOptions": {
    "jsx": "preserve",
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "snapp/*": ["node_modules/snapp/dist/app/*"]
    },
    "typeRoots": ["typings", "node_modules/@types"],
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
